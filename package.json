{
  "name": "flexybillz-mobile",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "dev": "export NODE_ENV=development && expo start --dev-client",
    "dev-win": "set NODE_ENV=development && expo start --dev-client",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "build:dev:android": "npx eas-cli@3.13.3 build --profile development --platform android",
    "build:dev:ios": "npx eas-cli@3.13.3 build --profile development --platform ios",
    "build:uat:android": "npx eas-cli@3.13.3 build --profile uat --platform android",
    "build:uat:ios": "npx eas-cli@3.13.3 build --profile uat --platform ios",
    "build:prod:android": "npx eas-cli@3.13.3 build --profile production --platform android",
    "build:prod:ios": "npx eas-cli@3.13.3 build --profile production --platform ios",
    "ci:dev": "npx eas-cli@3.13.3 build --profile development --platform all --no-wait --non-interactive",
    "ci:uat": "npx eas-cli@3.13.3 build --profile uat --platform all --no-wait --non-interactive",
    "ci:prod": "npx eas-cli@3.13.3 build --profile production --platform all --no-wait --non-interactive",
    "eas-build-pre-install": "node ./scripts/pre-install.js",
    "eas-build-on-success": "node ./scripts/post-install.js"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.18.2",
    "@react-native-masked-view/masked-view": "0.2.9",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/core": "^6.4.10",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "@reduxjs/toolkit": "^1.9.7",
    "@tanstack/react-query": "^5.7.2",
    "axios": "0.18",
    "expo": "~49.0.15",
    "expo-dev-client": "~2.4.11",
    "expo-font": "~11.4.0",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "formik": "^2.4.5",
    "js-base64": "^3.7.5",
    "react": "18.2.0",
    "react-native": "0.72.6",
    "react-native-dotenv": "^3.4.9",
    "react-native-element-dropdown": "^2.10.0",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-snap-carousel": "^3.9.1",
    "react-native-swipe-modal-up-down": "^1.1.0",
    "react-native-toast-notifications": "^3.4.0",
    "react-native-vector-icons": "^10.0.1",
    "react-redux": "^8.1.3",
    "styled-components": "^6.1.0",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.14",
    "@types/react-native-snap-carousel": "^3.8.7",
    "@types/react-native-vector-icons": "^6.4.16",
    "typescript": "^5.1.3"
  },
  "private": true
}
